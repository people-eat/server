{
    "name": "@people-eat/server",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "nx run-many --target=build",
        "graph": "nx graph",
        "lint": "eslint packages/**/**.ts",
        "npm-check": "npm-check -i @people-eat/*",
        "npm-check-all": "nx run-many --target=npm-check && yarn npm-check",
        "start": "node --loader ts-node/esm packages/adapter-host-development/src/index.ts",
        "start:prod": "node packages/adapter-host-development/dist/index.js",
        "sort:package:json": "sort-package-json package.json packages/**/package.json",
        "format": "prettier --write .",
        "verify": "yarn build"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^18.14.6",
        "@typescript-eslint/eslint-plugin": "^5.54.1",
        "@typescript-eslint/parser": "^5.54.1",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "npm-check": "^6.0.1",
        "nx": "^15.8.5",
        "prettier": "^2.8.4",
        "prettier-plugin-organize-imports": "^3.2.2",
        "sort-package-json": "^2.4.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": "^19.5.0",
        "npm": "please-use-yarn",
        "yarn": "^1.20.0"
    }
}
